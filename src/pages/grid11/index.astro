---
import Layout from '../../layouts/Layout.astro';
import GridOverlay from '../../components/Grid6/Overlay.astro';
---

<Layout>
  <span>ease-in-out-expo</span>
  <GridOverlay cubicBezier="1, 0, 0, 1" delayStep={0.15} />
  <style>
    span {
      position: absolute;
      top: 1rem;
      left: 1rem;
      z-index: 10;
      color: white;
      background-color: rgba(0, 0, 0, 0.5);
      padding: 0.25rem 0.5rem;
      border-radius: 4px;
      font-family: monospace;
    }
  </style>
</Layout>
